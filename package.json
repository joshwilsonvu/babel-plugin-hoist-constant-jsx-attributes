{
  "name": "babel-plugin-hoist-constant-jsx-attributes",
  "author": "Josh Wilson",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Prevents unnecessary rerenders and GC pressure by hoisting constant objects out of JSX attributes and into the top-level scope, ex. for objects in a 'style' prop.",
  "main": "dist/index.js",
  "module": "dist/babel-plugin-hoist-constant-jsx-attributes.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint --fix src test",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "publish": "np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "np": {
    "branch": "main"
  },
  "size-limit": [
    {
      "path": "dist/babel-plugin-hoist-constant-jsx-attributes.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/babel-plugin-hoist-constant-jsx-attributes.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/babel-core": "^6.25.6",
    "babel-plugin-tester": "^10.0.0",
    "husky": "^5.1.3",
    "size-limit": "^4.10.1",
    "ts-jest": "^26.5.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@babel/template": "^7.12.13",
    "@types/babel-traverse": "^6.25.5",
    "@types/babel-types": "^7.0.9",
    "@types/micromatch": "^4.0.1",
    "deep-equal": "^2.0.5",
    "micromatch": "^4.0.2"
  }
}
